<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/rl_contain"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:ignore="MissingPrefix"
    android:background="@drawable/app_bg"
    android:orientation="vertical"
    >
  <FrameLayout
      android:layout_width="match_parent"
      android:layout_height="64dp"
      android:layout_marginLeft="10dp"
      android:layout_marginTop="10dp"
      android:background="@drawable/ic_title_bg"
      android:orientation="horizontal"
      android:paddingRight="10dp"
      >
    <TextView
        android:id="@+id/txt_load_data"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
        android:text="PHYSICAL EXAMINATION"
        android:textColor="#ffffff"
        android:textSize="44sp"
        android:visibility="gone"
        />

    <TextView
        android:id="@+id/txt_m"
        android:layout_width="160dp"
        android:layout_height="64dp"
        android:background="@mipmap/ic_sex_bg"
        android:gravity="center"
        android:paddingRight="5dp"
        android:textColor="#ffffff"
        android:textSize="28sp"
        android:visibility="gone"
        />
    <TextView
        android:id="@+id/txt_a"
        android:layout_width="160dp"
        android:layout_height="64dp"
        android:background="@mipmap/ic_age_bg"
        android:gravity="center"
        android:paddingRight="5dp"
        android:textColor="#ffffff"
        android:textSize="28sp"
        android:visibility="gone"
        />
    <TextView
        android:id="@+id/txt_h"
        android:layout_width="160dp"
        android:layout_height="64dp"
        android:background="@mipmap/ic_height_bg"
        android:gravity="center"
        android:paddingRight="5dp"
        android:textColor="#ffffff"
        android:textSize="28sp"
        android:visibility="gone"
        />
    <TextView
        android:id="@+id/txt_w"
        android:layout_width="160dp"
        android:layout_height="match_parent"
        android:background="@mipmap/ic_weight_bg"
        android:gravity="center"
        android:paddingRight="5dp"
        android:textColor="#ffffff"
        android:textSize="28sp"
        />
    <TextView
        android:id="@+id/txt_i"
        android:layout_width="160dp"
        android:layout_height="match_parent"
        android:background="@mipmap/ic_id_bg"
        android:visibility="gone"
        android:paddingRight="5dp"
        android:textColor="#ffffff"
        android:textSize="28sp"
        />
    <LinearLayout
        android:id="@+id/ly_title_person_msg"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_gravity="right|center_vertical"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:visibility="gone"
        >
      <TextView
          android:id="@+id/txt_title_id"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_marginRight="64dp"
          android:shadowColor="#80000000"
          android:shadowDx="2"
          android:shadowDy="2"
          android:shadowRadius="2"
          android:text="ID"
          android:textColor="#ffffff"
          android:textSize="26.7sp"
          />
      <TextView
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_marginRight="15dp"
          android:shadowColor="#80000000"
          android:shadowDx="2"
          android:shadowDy="2"
          android:shadowRadius="2"
          android:text="身高"
          android:textColor="#ffffff"
          android:textSize="26.7sp"
          />
      <TextView
          android:id="@+id/txt_title_height"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_marginRight="64dp"
          android:shadowColor="#80000000"
          android:shadowDx="2"
          android:shadowDy="2"
          android:shadowRadius="2"
          android:text="176"
          android:textColor="#ffffff"
          android:textSize="24sp"
          />
      <TextView
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_marginRight="15dp"
          android:shadowColor="#80000000"
          android:shadowDx="2"
          android:shadowDy="2"
          android:shadowRadius="2"
          android:text="年龄"
          android:textColor="#ffffff"
          android:textSize="26.7sp"
          />
      <TextView
          android:id="@+id/txt_title_age"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_marginRight="64dp"
          android:shadowColor="#80000000"
          android:shadowDx="2"
          android:shadowDy="2"
          android:shadowRadius="2"
          android:text="26"
          android:textColor="#ffffff"
          android:textSize="24sp"
          />
      <TextView
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_marginRight="15dp"
          android:letterSpacing="-0.08"
          android:shadowColor="#80000000"
          android:shadowDx="2"
          android:shadowDy="2"
          android:shadowRadius="2"
          android:text="性别"
          android:textColor="#ffffff"
          android:textSize="26.7sp"
          />
      <TextView
          android:id="@+id/txt_title_sex"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:shadowColor="#80000000"
          android:shadowDx="2"
          android:shadowDy="2"
          android:shadowRadius="2"
          android:text="女"
          android:layout_marginRight="10dp"
          android:textColor="#ffffff"
          android:textSize="24sp"
          />
    </LinearLayout>
    <ImageView
        android:id="@+id/logo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="right|center_vertical"
        android:src="@mipmap/logo"
        tools:visibility="visible"
        />
  </FrameLayout>
  <FrameLayout
      android:id="@+id/fl_container"
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      android:layout_marginTop="64dp"
      android:paddingBottom="65dp"
      >
  </FrameLayout>



</RelativeLayout>
